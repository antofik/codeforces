                if ($(this).find(":disabled").length === 0) {
                    $("#facebox .blogEntry-9003-error").hide();
                    $("#facebox #blogEntry-9003-resource-submitting-indicator").show();
                    $("#facebox #blogEntry-9003-resource-submit").attr("disabled", "disabled");

                    //noinspection JSUnusedGlobalSymbols
                    $.post("/data/blogAndContest?action=attachBlogToContest", {
                        blogId:"6883", contestId:$("#facebox #blogEntry-9003-contestId").val(),
                        blogEntryId:"9003",
                        resourceType:$("#facebox #blogEntry-9003-resourceType").val()
                    }, function (json) {
                        var validated = true;

                        for (var key in json) {
                            if (json.hasOwnProperty(key) && key.indexOf("error__") === 0) {
                                validated = false;
                                $("#facebox .blogEntry-9003-" + key.substring("error__".length) + "-error").show();
                                $("#facebox .for__blogEntry-9003-" + key.substring("error__".length)).text(json[key]);
                            }
                        }

                        $("#facebox #blogEntry-9003-resource-submitting-indicator").hide();
                        $("#facebox #blogEntry-9003-resource-submit").removeAttr("disabled");

                        if (validated) {
                            $.facebox.close();
                            if (json["success"] === "true") {
                                Codeforces.showMessage("Блог был успешно прикреплён к выбранному соревнованию");
                            } else {
                                Codeforces.showMessage("Во время прикрепления блога к соревнованию произошла непредвиденная ошибка");
                            }
                        }
                    }, "json");
                }

                return false;
            });

            var contestSelect = $("#blogEntry-9003-contestId");
            var popupContestSelect = $("#facebox #blogEntry-9003-contestId");

            if (popupContestSelect.is(":disabled")) {
                $.post("/data/contests?action=getContestListWithEditableResources", function (data) {
                    if (data["result"] === "success") {
                        var contests = data["contests"];

                        for (var i = 0, contestCount = contests.length; i < contestCount; ++i) {
                            //noinspection StringLiteralBreaksHTMLJS
                            var contestOptionHtml = "<option value=\"" + contests[i]["id"] + "\">"
                                    + contests[i]["id"] + " - " + contests[i]["name"] + "</option>";
                            contestSelect.append(contestOptionHtml);
                            popupContestSelect.append(contestOptionHtml);
                        }

                        contestSelect.removeAttr("disabled");
                        $("#blogEntry-9003-contestId-loading-indicator").hide();
                        $("#blogEntry-9003-resource-submit").removeAttr("disabled");

                        popupContestSelect.removeAttr("disabled");
                        $("#facebox #blogEntry-9003-contestId-loading-indicator").hide();
                        $("#facebox #blogEntry-9003-resource-submit").removeAttr("disabled");
                    } else {
                        alert("Не удалось получить список соревнований");
                    }
                }, "json");
            }

            return false;
        });
    });
</script>


        <span style="position:absolute;right:0;top:0.25em;margin-right:1em;display:inline;font-size:0.75em;">
            <div style="margin-top:0.25em;">
            </div>
        </span>
    </div>

    <div class="content">
        <div class="ttypography"><p>Всем привет! </p><p>Скоро (<a href="http://www.timeanddate.com/worldclock/fixedtime.html?day=1&amp;month=10&amp;year=2013&amp;hour=19&amp;min=30&amp;sec=0&amp;p1=166">1 октября, 19:30 MSK</a>) состоится очередной Codeforces Round #203 для участников Div. 2. Как обычно, Div. 1 участники смогут поучаствовать в этом раунде вне конкурса.</p><p>Автором раунда являюсь я. Хочется сказать большое спасибо Гере Агапову (<a class="rated-user user-red" href="/profile/Gerald" title="Гроссмейстер Gerald">Gerald</a>) за помощь в подготовке и за хорошие предложения по задачам. Спасибо Лось Илье (<a class="rated-user user-orange" href="/profile/IlyaLos" title="Мастер IlyaLos">IlyaLos</a>) за тестирование задач, Марии Беловой (<a class="rated-user user-black" href="/profile/Delinur" title="Не в рейтинге, Delinur">Delinur</a>) за переводы на английский, Михаилу Мирзаянову (<a class="rated-user user-admin" href="/profile/MikeMirzayanov" title="Штаб, MikeMirzayanov">MikeMirzayanov</a>) за замечательные системы Codeforces и Polygon. Также спасибо Игнатьеву Александру (<a class="rated-user user-blue" href="/profile/dudkamaster" title="Эксперт dudkamaster">dudkamaster</a>) за тестирование задач и идею одной из них.</p><p>Удачи!</p><p><strong>UPD</strong>: Будет использоваться динамическое распределение баллов. Задачи расположены в порядке увеличения сложности (по мнению авторов).</p><p><strong>UPD</strong>: Поздравляем победителей!</p> <ol>   <li><a class="rated-user user-black" href="/profile/fanhqme" title="Не в рейтинге, fanhqme">fanhqme</a></li>   <li><a class="rated-user user-black" href="/profile/FAU.COACH" title="Не в рейтинге, FAU.COACH">FAU.COACH</a></li>   <li><a class="rated-user user-blue" href="/profile/Witalia" title="Эксперт Witalia">Witalia</a></li>   <li><a class="rated-user user-blue" href="/profile/sokian" title="Эксперт sokian">sokian</a></li> </ol><p><strong>UPD:</strong> <a href="http://codeforces.ru/blog/entry/9042">Разбор</a></p></div><p><a href="/blog/entry/9003">Полный текст &raquo;</a></p>
