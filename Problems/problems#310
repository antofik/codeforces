<script type="text/javascript">
    $(document).ready(function () {
        $(".blogEntry-9003-error").hide();

        $(".attach-blogEntry-9003-to-contest-link").click(function () {
            Codeforces.facebox(".attach-blogEntry-9003-to-contest-form-holder", "http://worker.codeforces.ru/static");

