<script type="text/javascript">
    $(document).ready(function () {
        $(".blogEntry-8985-error").hide();

        $(".attach-blogEntry-8985-to-contest-link").click(function () {
            Codeforces.facebox(".attach-blogEntry-8985-to-contest-form-holder", "http://worker.codeforces.ru/static");

            $("#facebox .attach-blogEntry-8985-to-contest-form").submit(function () {
                if ($(this).find(":disabled").length === 0) {
                    $("#facebox .blogEntry-8985-error").hide();
                    $("#facebox #blogEntry-8985-resource-submitting-indicator").show();
                    $("#facebox #blogEntry-8985-resource-submit").attr("disabled", "disabled");

                    //noinspection JSUnusedGlobalSymbols
                    $.post("/data/blogAndContest?action=attachBlogToContest", {
                        blogId:"784", contestId:$("#facebox #blogEntry-8985-contestId").val(),
                        blogEntryId:"8985",
                        resourceType:$("#facebox #blogEntry-8985-resourceType").val()
                    }, function (json) {
                        var validated = true;

                        for (var key in json) {
                            if (json.hasOwnProperty(key) && key.indexOf("error__") === 0) {
                                validated = false;
                                $("#facebox .blogEntry-8985-" + key.substring("error__".length) + "-error").show();
                                $("#facebox .for__blogEntry-8985-" + key.substring("error__".length)).text(json[key]);
                            }
                        }

                        $("#facebox #blogEntry-8985-resource-submitting-indicator").hide();
                        $("#facebox #blogEntry-8985-resource-submit").removeAttr("disabled");

                        if (validated) {
                            $.facebox.close();
                            if (json["success"] === "true") {
                                Codeforces.showMessage("Блог был успешно прикреплён к выбранному соревнованию");
                            } else {
                                Codeforces.showMessage("Во время прикрепления блога к соревнованию произошла непредвиденная ошибка");
                            }
                        }
                    }, "json");
                }

                return false;
            });

            var contestSelect = $("#blogEntry-8985-contestId");
            var popupContestSelect = $("#facebox #blogEntry-8985-contestId");

            if (popupContestSelect.is(":disabled")) {
                $.post("/data/contests?action=getContestListWithEditableResources", function (data) {
                    if (data["result"] === "success") {
                        var contests = data["contests"];

                        for (var i = 0, contestCount = contests.length; i < contestCount; ++i) {
                            //noinspection StringLiteralBreaksHTMLJS
                            var contestOptionHtml = "<option value=\"" + contests[i]["id"] + "\">"
                                    + contests[i]["id"] + " - " + contests[i]["name"] + "</option>";
                            contestSelect.append(contestOptionHtml);
                            popupContestSelect.append(contestOptionHtml);
                        }

                        contestSelect.removeAttr("disabled");
                        $("#blogEntry-8985-contestId-loading-indicator").hide();
                        $("#blogEntry-8985-resource-submit").removeAttr("disabled");

                        popupContestSelect.removeAttr("disabled");
                        $("#facebox #blogEntry-8985-contestId-loading-indicator").hide();
                        $("#facebox #blogEntry-8985-resource-submit").removeAttr("disabled");
                    } else {
                        alert("Не удалось получить список соревнований");
                    }
                }, "json");
            }

            return false;
        });
    });
</script>


        <span style="position:absolute;right:0;top:0.25em;margin-right:1em;display:inline;font-size:0.75em;">
            <div style="margin-top:0.25em;">
            </div>
